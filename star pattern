for row in range(0,7,1):
    for col in range(0,11,1):
        if((col==0)or(row==0 and col<4) or(row==6 and col<4) or (col==4 and row>0 and row<6)):
            print("*",end=" ")
        else:
            print(" ",end=" ")
        if((col==6 and row>0 and row<6)or (col==10 and row>0 and row<6)or(row==0 and col>6 and col<10) or (row==6 and col>6 and col<10)):
            print("*",end=" ")
        else:
            print(" ",end=" ")
    print()
